{
  "name": "WadateAppV2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:dev": "npm-run-all generate:gql --parallel start generate:gql:watch",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky install",
    "link": "react-native link",
    "generate:gql:watch": "npm-watch generate:gql",
    "generate:schema": "rm -rf src/graphql/generated && npx apollo service:download --endpoint='https://api-dev-wedate.westeurope.azurecontainer.io/graphql' --skipSSLValidation schema.json && npm run generate:gql",
    "generate:gql": "graphql-codegen --config .graphqlrc.yml"
  },
  "watch": {
    "generate:gql": {
      "patterns": [
        "src"
      ],
      "extensions": "graphql",
      "quiet": true,
      "runOnChangeOnly": true
    }
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@react-native-aria/overlays": "^0.3.2",
    "@react-native-community/datetimepicker": "^6.1.3",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.2",
    "@shopify/restyle": "^2.1.0",
    "@types/jest": "^28.1.6",
    "@types/react-native-dotenv": "^0.2.0",
    "apollo-boost": "^0.4.9",
    "axios": "^0.27.2",
    "axios-auth-refresh": "^3.3.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.28.0",
    "graphql": "^15.8.0",
    "hoist-non-react-statics": "^3.3.2",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.3",
    "prop-types": "^15.8.1",
    "react": "18.0.0",
    "react-devtools-core": "^4.24.3",
    "react-hook-form": "^7.34.0",
    "react-native": "0.69.0",
    "react-native-date-picker": "^4.2.1",
    "react-native-deck-swiper": "^2.0.7",
    "react-native-dotenv": "^3.3.1",
    "react-native-fast-image": "^8.5.11",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-image-picker": "^4.8.5",
    "react-native-keychain": "^8.1.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal-datetime-picker": "^13.1.2",
    "react-native-network-logger": "^1.13.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@graphql-codegen/cli": "2.9.1",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/apollo-upload-client": "^17.0.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/react": "^18.0.8",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.6.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18.0.8"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
